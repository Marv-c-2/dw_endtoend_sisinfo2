{
	"name": "DF_SilverToSQL_Products",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Products_Silver",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_DimProduct_SQL",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1",
					"description": "Agregue una expresiÃ³n para el filtro."
				}
			],
			"scriptLines": [
				"source(output(",
				"          product_id as string,",
				"          product_name as string,",
				"          category_id as string,",
				"          launch_date as string,",
				"          price as string,",
				"          notes as string,",
				"          launch_date_normalized as date,",
				"          product_id_clean as string,",
				"          price_clean as string,",
				"          launch_date_clean as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          product_name,",
				"          category = category_id,",
				"          product_id = product_id_clean,",
				"          price = price_clean,",
				"          launch_date = launch_date_clean",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(!isNull(category) && !isNull(price)",
				") ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          product_id as string,",
				"          product_name as string,",
				"          category_id as string,",
				"          price as integer,",
				"          launch_date as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          product_id,",
				"          product_name,",
				"          category_id = category,",
				"          price,",
				"          launch_date",
				"     )) ~> sink1"
			]
		}
	}
}